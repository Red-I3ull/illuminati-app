[run]
source = .
omit =
    */migrations/*
    */tests.py
    */test_*.py
    */__pycache__/*
    */venv/*
    */.venv/*
    */env/*
    */.env/*
    */manage.py
    */__init__.py
    */settings.py
    */wsgi.py
    */asgi.py
    */auth_backend.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
precision = 2
show_missing = True

[html]
directory = htmlcov
